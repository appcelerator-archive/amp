{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": {
          "enabled": false
        }
      },
      "_meta": {
        "version": "5.3.1"
      },
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "name": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "version": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "container_id": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "container_name": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "container_short_name": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "container_state": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "fail_count": {
          "type": "long"
        },
        "fields": {
          "properties": {}
        },    
        "limit": {
          "type": "long"
        },
        "max_usage": {
          "type": "long"
        },
        "meta": {
          "properties": {
            "cloud": {
              "properties": {
                "availability_zone": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "instance_id": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "machine_type": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "project_id": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "provider": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                },
                "region": {
                  "ignore_above": 1024,
                  "index": "not_analyzed",
                  "type": "string"
                }
              }
            }
          }
        },
        "msg": {
          "index": "analyzed",
          "norms": {
            "enabled": false
          },
          "type": "string"
        },
        "node_id": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "read": {
          "type": "long"
        },
        "role": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "rx_bytes": {
          "type": "long"
        },
        "rx_dropped": {
          "type": "long"
        },
        "rx_errors": {
          "type": "long"
        },
        "rx_packets": {
          "type": "long"
        },
        "service_id": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "service_name": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "stack_name": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "tags": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "task_id": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "total": {
          "type": "long"
        },
        "total_bytes": {
          "type": "long"
        },
        "total_usage": {
          "type": "double"
        },
        "tx_bytes": {
          "type": "long"
        },
        "tx_dropped": {
          "type": "long"
        },
        "tx_errors": {
          "type": "long"
        },
        "tx_packets": {
          "type": "long"
        },
        "usage": {
          "type": "long"
        },
        "usage_in_kernel_mode": {
          "type": "double"
        },
        "usage_in_user_mode": {
          "type": "double"
        },
        "usage_pct": {
          "type": "double"
        },
        "write": {
          "type": "long"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "ampbeat-*"
}
